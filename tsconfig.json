{
  "compilerOptions": {
    "target": "ES5", // 想要的编译后的结果
    "module": "NodeNext", // 使用什么模块处理模式
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "allowImportingTsExtensions": true,
    "noEmit": true,
    "emitDeclarationOnly": false
  }
}
